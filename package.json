{
  "name": "spartis-application",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:react dev:server",
    "dev:react": "vite",
    "dev:server": "nodemon --watch \"server/**/*.ts\" --exec \"node --loader ts-node/esm server/server.ts\"",
    "start": "node server/build/server.js",
    "build": "npm run build:react && npm run build:server",
    "build:react": "vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@google/model-viewer": "^4.1.0",
    "@kitware/vtk.js": "^34.6.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-three/drei": "^10.3.0",
    "@react-three/fiber": "^9.1.2",
    "@types/three": "^0.177.0",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "express": "^4.19.2",
    "lucide-react": "^0.513.0",
    "multer": "^2.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.6.2",
    "tailwind-merge": "^3.3.0",
    "three": "^0.172.0",
    "three-stdlib": "^2.36.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@tailwindcss/postcss": "^4.1.8",
    "@types/express": "^4.17.21",
    "@types/multer": "^2.0.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.3.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
